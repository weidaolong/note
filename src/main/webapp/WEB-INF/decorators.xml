<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts">
	<excludes>
		<pattern>/static/*</pattern>
	</excludes>
	
	<decorator name="set" page="setting.jsp">
		<pattern>/bg*/*</pattern>
		<pattern>/bg*</pattern>
		<pattern>/set</pattern>
	</decorator>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
	
</decorators>
